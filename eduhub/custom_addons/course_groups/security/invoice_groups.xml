<odoo>
    <record id="eduhub_category" model="ir.module.category">
        <field name="name">EduHub Category</field>
    </record>

    <record id="group_eduhub_administrator" model="res.groups">
        <field name="name">Administrator</field>
        <field name="category_id" ref="eduhub_category"/>
    </record>

    <record id="group_eduhub_admin" model="res.groups">
        <field name="name">Admin</field>
        <field name="category_id" ref="eduhub_category"/>
    </record>

    <record id="access_eduhub_invoice_admin" model="ir.model.access">
        <field name="name">access_eduhub_invoice_admin</field>
        <field name="model_id" ref="course_groups.model_eduhub_invoice"/>
        <field name="group_id" ref="group_eduhub_admin"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>

    <record id="access_eduhub_invoice_administrator" model="ir.model.access">
        <field name="name">access_eduhub_invoice_administrator</field>
        <field name="model_id" ref="course_groups.model_eduhub_invoice"/>
        <field name="group_id" ref="group_eduhub_administrator"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="0"/>
        <field name="perm_create" eval="0"/>
        <field name="perm_unlink" eval="0"/>
    </record>

    <record id="access_eduhub_invoice_payment_admin" model="ir.model.access">
        <field name="name">access_eduhub_invoice_payment_admin</field>
        <field name="model_id" ref="course_groups.model_eduhub_invoice_payment"/>
        <field name="group_id" ref="group_eduhub_admin"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="1"/>
        <field name="perm_create" eval="1"/>
        <field name="perm_unlink" eval="1"/>
    </record>

    <record id="access_eduhub_invoice_payment_administrator" model="ir.model.access">
        <field name="name">access_eduhub_invoice_payment_administrator</field>
        <field name="model_id" ref="course_groups.model_eduhub_invoice_payment"/>
        <field name="group_id" ref="group_eduhub_administrator"/>
        <field name="perm_read" eval="1"/>
        <field name="perm_write" eval="0"/>
        <field name="perm_create" eval="0"/>
        <field name="perm_unlink" eval="0"/>
    </record>
</odoo>
